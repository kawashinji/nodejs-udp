{
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "extends": [
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": false,
    "no-boolean-literal-compare": false,
    "import-name": false,
    "prettier": [
      true,
      {
        "singleQuote": true,
        "semi": false
      }
    ],
    "object-shorthand-properties-first": true,
    "ordered-imports": true,
    "radix": false
  }
}
